		<env.conf.as.hostName>${env.hostName}</env.conf.as.hostName>
		<env.conf.as.ip>${env.ip}</env.conf.as.ip>
		<env.conf.as.port>8080</env.conf.as.port>
		<env.conf.as.protocol>http</env.conf.as.protocol>
		<env.conf.as.userName>${pkg.code}</env.conf.as.userName>
		<env.conf.as.password>change_or_override_me</env.conf.as.password>
		<env.conf.as.contextRoot><!-- Default to none --></env.conf.as.contextRoot> <!-- With leading forward-slash if any -->
		<env.conf.as.contextPath>/${pkg.name}</env.conf.as.contextPath> <!-- Relative to contextRoot -->
		
		<env.conf.db.type>mysql</env.conf.db.type> <!-- Possible Types: mysql, oracle, postgresql, odbc, db2, sqlserver -->
		<env.conf.db.hostName>${env.hostName}</env.conf.db.hostName>
		<env.conf.db.ip>${env.ip}</env.conf.db.ip>
		<env.conf.db.port>3306</env.conf.db.port>
		<env.conf.db.userName>${pkg.code}</env.conf.db.userName>
		<env.conf.db.password>change_or_override_me</env.conf.db.password>
		<env.conf.db.schema>${pkg.code}</env.conf.db.schema>
		<env.conf.db.charset>UTF-8</env.conf.db.charset>
		<env.conf.db.isDebug>true</env.conf.db.isDebug>
		<env.conf.db.isProfile>false</env.conf.db.isProfile>
		
		<env.conf.db.jdbc.driver>com.mysql.jdbc.Driver</env.conf.db.jdbc.driver>
		<env.conf.db.jdbc.url>jdbc:${env.conf.db.type}://${env.conf.db.hostName}:${env.conf.db.port}/${env.conf.db.schema}?characterEncoding=${env.conf.db.charset}&amp;amp;profileSQL=${env.conf.db.isProfile}</env.conf.db.jdbc.url>

