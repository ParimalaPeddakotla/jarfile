		<!-- Deployment Environment codes and environment specific configurations -->				
		<env.code>DEV</env.code> <!-- Possible Codes: DEV, TST, UAT, FIX, PRF, PRP, PRD -->
		<env.type>dev</env.type> <!-- For Tomcat Plugin, Remove it! -->
		<env.name>Development</env.name>
		<env.hostName>${env.type}</env.hostName>
		<env.ip>127.0.0.1</env.ip>
		<env.description><!-- EMPTY --></env.description>
		
		<!-- Environment specific (AS, DB etc.) configurations -->
