									<Implementation-Version>${pkg.version.number}-${pkg.version.buildNumber}</Implementation-Version>
									<Implementation-Vendor>${org.fullName}</Implementation-Vendor>
									<Implementation-Vendor-Id>${org.vendorId}</Implementation-Vendor-Id>
									<Implementation-Vendor-URL>${org.url}</Implementation-Vendor-URL>
									<Implementation-URL>${pkg.url}</Implementation-URL>
									
									<Package-Title>${pkg.title}</Package-Title>
									<Package-Name>${pkg.name}</Package-Name>
									<Package-Code>${pkg.code}</Package-Code>							
									<Package-Url>${pkg.url}</Package-Url>
									<Package-Version>${pkg.version.string}</Package-Version>
									<Package-Vendor>${org.fullName}</Package-Vendor>
									<Package-Technology>${pkg.technology.code}</Package-Technology>
									<Package-Product>${pkg.product.code}</Package-Product>
								</manifestEntries>
							</archive>
						</configuration>
					</plugin>				
				</plugins>
			</build>
		</profile>
		
		<profile>
			<id>report-generate</id>
			
			<activation>
				<property><name>generateReports</name></property>
			</activation>
			
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-site-plugin</artifactId>
						<version>3.4</version>
						
						<dependencies>
							<dependency>
								<groupId>org.apache.maven.doxia</groupId>
								<artifactId>doxia-module-xhtml</artifactId>
								<version>1.5</version>
							</dependency>
							
							<dependency>
								<groupId>org.apache.maven.doxia</groupId>
								<artifactId>doxia-module-confluence</artifactId>
								<version>1.5</version>
							</dependency>
							
							<dependency>
								<groupId>org.apache.maven.doxia</groupId>
								<artifactId>doxia-module-rtf</artifactId>
								<version>1.5</version>
							</dependency>
						</dependencies>
						
						<executions>
							<execution>
								<goals>
									<goal>site</goal>
									<goal>deploy</goal>
								</goals>
								
								<phase>deploy</phase> 
							</execution>
						</executions>
						<configuration>
							<outputEncoding>${project.reporting.outputEncoding}</outputEncoding>
							<!--<locales>en,tr</locales>-->
						</configuration>
					</plugin>				
				</plugins>
			</build>
			
			<reporting>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-project-info-reports-plugin</artifactId>
						<version>2.8</version>
						
						<configuration>
							<dependencyLocationsEnabled>false</dependencyLocationsEnabled><!-- waiting for MPIR-267 -->
							<customBundle>${project.basedir}/src/site/custom/project-info-report.properties</customBundle>
						</configuration>
						
						<reportSets>
							<reportSet>
								<reports>
									<report>summary</report>
									<report>dependency-info</report>
									<report>modules</report>
									<report>license</report>
									<report>project-team</report>
									<report>scm</report>
									<report>issue-tracking</report>
									<report>mailing-list</report>
									<report>dependency-management</report>
									<report>dependencies</report>
									<report>dependency-convergence</report>
									<report>cim</report>
									<report>plugin-management</report>
									<report>plugins</report>
									<report>distribution-management</report>
								</reports>
							</reportSet>
						</reportSets>
					</plugin>
					
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>sonar-maven-plugin</artifactId>
						<version>2.6</version>
					</plugin>
					
					<plugin>
						<groupId>org.codehaus.sonar-plugins</groupId>
						<artifactId>maven-report</artifactId>
						<version>0.1</version>
					</plugin>
					
					<plugin>
						<groupId>org.jacoco</groupId>
						<artifactId>jacoco-maven-plugin</artifactId>
						<version>0.7.0.201403182114</version>
					</plugin>
					
					<plugin> 
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-surefire-report-plugin</artifactId>
						<version>2.17</version>
					</plugin>
					
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-checkstyle-plugin</artifactId>
						<version>2.13</version>
					</plugin>
					
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-changelog-plugin</artifactId>
						<version>2.3</version>
						<configuration>
							<type>range</type>
							<range>30</range>
							<headingDateFormat>dd.MM.yyyy</headingDateFormat>
						</configuration>
					</plugin>
					
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-pmd-plugin</artifactId>
						<version>3.2</version>
						<configuration>
						</configuration>
					</plugin>

					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>taglist-maven-plugin</artifactId>
						<version>2.4</version>
						
						<configuration>
							<encoding>${pkg.build.sourceEncoding}</encoding>
							<sourceFileLocale>en</sourceFileLocale>
							<emptyComments>false</emptyComments>
							<aggregate>true</aggregate>
							
							<tagListOptions>
								<tagClasses>
									<tagClass>
										<displayName>Documentation Work</displayName>
										
										<tags>
											<tag>
												<matchString>DOCUMENT_ME</matchString>
												<matchType>ignoreCase</matchType>
											</tag>
											
											<tag>
												<matchString>NOT_YET_DOCUMENTED</matchString>
												<matchType>ignoreCase</matchType>
											</tag>
										</tags>
									</tagClass>
									
									<tagClass>
										<displayName>Code Work</displayName>
										
										<tags>
											<tag>
												<matchString>&lt;todo</matchString>
												<matchType>ignoreCase</matchType>
											</tag>
											
											<tag>
												<matchString>TODO</matchString>
												<matchType>ignoreCase</matchType>
											</tag>
											
											<tag>
												<matchString>@TODO</matchString>
												<matchType>ignoreCase</matchType>
											</tag>
										</tags>
									</tagClass>
									
									<tagClass>
										<displayName>Code Fix</displayName>
										
										<tags>
											<tag>
												<matchString>FIXME</matchString>
												<matchType>ignoreCase</matchType>
											</tag>
											
											<tag>
												<matchString>@FIXME</matchString>
												<matchType>ignoreCase</matchType>
											</tag>
										</tags>
									</tagClass>
								</tagClasses>
							</tagListOptions>
						</configuration>
					</plugin>
					
					<plugin>
						<groupId>com.atlassian.maven.plugins</groupId>
						<artifactId>maven-clover2-plugin</artifactId>
						<version>3.2.2</version>
						
						<configuration>
							<generatePdf>true</generatePdf>
							<generateXml>true</generateXml>
							<generateHtml>true</generateHtml>
							<generateJson>true</generateJson>
						</configuration>
					</plugin>
				</plugins>
			</reporting>
		</profile>
	</profiles>
	
