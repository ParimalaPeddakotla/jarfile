	<!-- Theese are generic properties that may be used by child POMs. -->
	<!-- They should be overridden by child POMs properly *as needed*. -->
	<properties>
		<!-- Basic package properties -->
		<pkg.title>Unknown Software Package</pkg.title>
		<pkg.name>${project.artifactId}</pkg.name>
		<pkg.code>UNKNOWN</pkg.code>
		<pkg.url>${pkg.links.homepage}</pkg.url>
		<pkg.description><!-- EMPTY --></pkg.description>
		<pkg.product.code>UNKNOWN</pkg.product.code>
		<pkg.technology.code>JAVA</pkg.technology.code>

		<!-- Version related properties -->
		<pkg.version.number>${project.version}</pkg.version.number>
		<pkg.version.qualifier><!-- EMPTY --></pkg.version.qualifier>
		<pkg.version.buildNumber>0</pkg.version.buildNumber>
		<pkg.version.string>${pkg.version.number}</pkg.version.string>
		
		<!-- Project links for configuration management environments -->
		<pkg.links.homepage>${app.common.baseUrl}/confluence/display/${pkg.product.code}</pkg.links.homepage>
		<pkg.links.ci>${app.common.baseUrl}/bamboo/browse/${pkg.product.code}</pkg.links.ci>
		<pkg.links.issue>${app.common.baseUrl}/jira/browse/${pkg.product.code}</pkg.links.issue>
		<pkg.links.scm>scm:git:ssh://git@${app.common.hostName}:7999/${pkg.product.code}/${pkg.name}.git</pkg.links.scm>
		<pkg.links.scm.dev>scm:git:ssh://git@${app.common.hostName}:7999/${pkg.product.code}/${pkg.name}.git</pkg.links.scm.dev>
		<pkg.links.scm.url>${app.common.baseUrl}/fecru/changelog/${pkg.name}</pkg.links.scm.url>	
		
		<!-- Build related properties -->
		<pkg.build.sourceEncoding>UTF-8</pkg.build.sourceEncoding>
		<pkg.build.javaVersion>1.7</pkg.build.javaVersion>
		<pkg.build.sourceJavaVersion>${pkg.build.javaVersion}</pkg.build.sourceJavaVersion>
		<pkg.build.targetJavaVersion>${pkg.build.javaVersion}</pkg.build.targetJavaVersion>
		
		<!-- Organization related properties -->
		<org.name>OBSS</org.name>
		<org.fullName>Open Business Software Solutions</org.fullName>
		<org.url>http://obss.com.tr/</org.url>
		<org.vendorId>tr.com.obss</org.vendorId>
		
		<!-- Unit Test Coverage Policy -->
		<pol.utest.classRatio>0</pol.utest.classRatio>
		<pol.utest.instructionRatio>0</pol.utest.instructionRatio>
		<pol.utest.methodRatio>0</pol.utest.methodRatio>
		<pol.utest.branchRatio>0</pol.utest.branchRatio>
		<pol.utest.complexityRatio>0</pol.utest.complexityRatio>
		<pol.utest.lineRatio>0</pol.utest.lineRatio>
		
